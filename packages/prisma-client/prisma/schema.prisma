generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../../../votesys.db"
}

model Voter {
  id            Int    @id @default(autoincrement())
  idNumber      String @unique
  name          String
  sex           String // male/female/apacheHelicopter
  age           Int
  state         String
  publicKeyRSA  String
  privateKeyRSA String
}

model Vote {
  id           Int    @id @default(autoincrement())
  vote         String // DEM/REP
  sex          String
  age          Int
  state        String
  signatureRSA String
}
